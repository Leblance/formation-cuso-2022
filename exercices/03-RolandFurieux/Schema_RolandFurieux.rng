   <define name="msDesc">
      <element name="msDesc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(description d'un manuscrit) contient la description d'un manuscrit individuel [10.1. Overview]</a:documentation>
         <group>
            <ref name="msIdentifier"/>
            <zeroOrMore>
               <ref name="model.headLike"/>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <ref name="model.pLike"/>
               </oneOrMore>
               <group>
                  <optional>
                     <ref name="msContents"/>
                  </optional>
                  <optional>
                     <ref name="physDesc"/>
                  </optional>
                  <optional>
                     <ref name="history"/>
                  </optional>
                  <optional>
                     <ref name="additional"/>
                  </optional>
                  <choice>
                     <zeroOrMore>
                        <ref name="msPart"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="msFrag"/>
                     </zeroOrMore>
                  </choice>
               </group>
            </choice>
         </group>
         <ref name="att.global.attributes"/>
         <ref name="att.sortable.attributes"/>
         <ref name="att.typed.attributes"/>
         <ref name="att.docStatus.attributes"/>
